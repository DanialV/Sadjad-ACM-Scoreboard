<br/>
<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <table id="user_verify" class="table table-bordered tableSorter fade-handel">
            <thead>
            <tr class="info">
                <th>Row</th>
                <th></span>Codeforces Username</th>
                <th>Accept</th>
                <th>Reject</th>
            </tr>
            </thead>
            <tbody>
            <% for(var i=0 ; i<data.length ; i++){ %>
            <tr>
                <td> <%- (i+1)%> </td>
                <td><a href="http://codeforces.com/profile/<%- data[i].codeforces_username%>"><%- data[i].codeforces_username%> </a></td>
                <td><button id="accept_<%-data[i].codeforces_username%>" type="button" class="btn btn-success">Accept</button></td>
                <td><button id="reject_<%-data[i].codeforces_username%>" type="button" class="btn btn-danger">Reject</button></td>
            </tr>
            <% }%>
            </tbody>
        </table>
    </div>
    <div class="col-md-2"></div>
</div>
